{
  "crons": [
    {
      "path": "/api/admin-generate-drafts?schedule=auto&track=multi&count=4&excludeSections=health,entertainment,technology",
      "schedule": "5 * * * *"
    },
    {
      "path": "/api/admin-generate-drafts?schedule=auto&track=single&count=3&includeSections=health,entertainment,technology",
      "schedule": "5 * * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
